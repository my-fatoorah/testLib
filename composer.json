{
    "name": "myfatoorah/lib-test",
    "description": "MyFatoorah PHP Library",
    "version": "2.0.0",
    "type": "library",
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    
    "keywords": [
        "MyFatoorah",
        "My Fatoorah",
        "Fatoorah",
        "gateway",
        "payment",
        "Shipping",
        "api"
    ],
    "homepage": "https://myfatoorah.com/",
    "authors": [
        {
            "name": "MyFatoorah Plugin Team"
        }
    ],
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
    	"files": ["autoload.php"],
        "psr-4": {
            "MyFatoorah\\Library\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyFatoorah\\Test\\": "tests"
        }
    }
}
